<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal">

        <ImageView
            android:id="@+id/file_upload_activity_iv_user_picture0"
            android:layout_width="62dp"
            android:layout_height="120dp"
            android:tag="0"
            android:src="@drawable/add_image" />

        <ImageView
            android:id="@+id/file_upload_activity_iv_user_picture1"
            android:layout_width="62dp"
            android:layout_height="120dp"
            android:tag="1"
            android:src="@drawable/add_image" />

        <ImageView
            android:id="@+id/file_upload_activity_iv_user_picture2"
            android:layout_width="62dp"
            android:layout_height="120dp"
            android:tag="2"
            android:src="@drawable/add_image" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal">

        <ImageView
            android:id="@+id/file_upload_activity_iv_user_picture3"
            android:layout_width="62dp"
            android:layout_height="120dp"
            android:tag="3"
            android:src="@drawable/add_image" />

        <ImageView
            android:id="@+id/file_upload_activity_iv_user_picture4"
            android:layout_width="62dp"
            android:layout_height="120dp"
            android:tag="4"
            android:src="@drawable/add_image" />

        <ImageView
            android:id="@+id/file_upload_activity_iv_user_picture5"
            android:layout_width="62dp"
            android:layout_height="120dp"
            android:tag="5"
            android:src="@drawable/add_image" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal">


        <Button
            android:id="@+id/file_upload_activity_btn_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="뒤로 가기" />

        <Button
            android:id="@+id/file_upload_activity_btn_regist"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="등 록" />

    </LinearLayout>

</LinearLayout>