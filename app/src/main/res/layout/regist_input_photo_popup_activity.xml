<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/white"
    tools:context=".registInputPhotoPopup.RegistInputPhotoPopupActivity">

    <LinearLayout
        android:id="@+id/regist_input_photo_popup_activity_linearlayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/regist_input_photo_popup_activity_tv_title"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:text="Notice"
            android:textAlignment="center"
            android:gravity="center"
            style="@style/normal_font_style"
            android:background="@drawable/btn_fill_popup_top_rectangle_green"
            android:textColor="@color/basic_font_color"
            />

        <Button
            android:id="@+id/regist_input_photo_popup_activity_btn_upload_Image"
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:text="사진 등록"
            style="@style/normal_font_style"
            android:background="@drawable/btn_fill_rectangle_white"
            android:textColor="@color/black"
            />

        <Button
            android:id="@+id/regist_input_photo_popup_activity_btn_delete_image"
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:text="사진 삭제"
            style="@style/normal_font_style"
            android:background="@drawable/btn_fill_rectangle_white"
            android:textColor="@color/black"
            />

        <Button
            android:id="@+id/regist_input_photo_popup_activity_btn_cancle"
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:text="취 소"
            style="@style/normal_font_style"
            android:background="@drawable/btn_fill_popup_bottom_rectangle_white"
            android:textColor="@color/main_color_green"
            />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>